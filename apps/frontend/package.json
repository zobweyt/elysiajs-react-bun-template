{
  "name": "@acme/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "bun build src/app/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"'",
    "check": "biome check",
    "clean": "rm -rf dist node_modules",
    "dev": "bun run ./scripts/serve.ts",
    "format": "biome format --write",
    "postinstall": "cp -n .env.example .env || true",
    "lint": "biome lint",
    "start": "NODE_ENV=production bun ./scripts/serve.ts",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.11",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.10.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
